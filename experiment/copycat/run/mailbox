#! /bin/bash

export RUST_LOG=info
export RUST_BACKTRACE=1

BUILDTYPE=$1
ID=$2
THREADS=$3
MAILBOX_WORKERS=$4
CONN_MULTIPLY=$5

./blockchain_copycat/target/${BUILDTYPE}/copycat_mailbox -i ${ID} -t ${THREADS} \
    -m bench_machines.json -n bench_network.json -w ${MAILBOX_WORKERS} -o ${CONN_MULTIPLY}
